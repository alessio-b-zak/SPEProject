<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:padding="15dp">

    <ImageView
        android:layout_width="100sp"
        android:layout_height="25dp"
        android:id="@+id/drawer_epimorphics_logo"
        android:src="@drawable/epimorphics_logo_white"
        android:layout_alignParentStart="false"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/drawer_uob_logo"
        android:layout_width="83dp"
        android:layout_height="25dp"
        android:layout_alignTop="@id/drawer_epimorphics_logo"
        android:layout_toStartOf="@id/drawer_epimorphics_logo"
        android:src="@drawable/uob_logo_white"
        android:layout_marginEnd="16dp"/>
</RelativeLayout>
